{
    "version": 2,
    "commands": [
      "python3 -m venv venv",
      "source venv/bin/activate",
      "pip install -r requirements.txt"      
    ],
    "builds": [
      { "src": "app_run_flask.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/app_run_flask.py" }
    ]
  }